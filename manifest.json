{
  "name": "Time and Weather Tab",
  "description": "Minimal New Tab Page.",
  "version": "0.3",
  "incognito": "split",
  "chrome_url_overrides": {"newtab": "start-page.html"},
  "background": {
    "scripts": ["jquery.min.js", "weather.js"]
  },
  "permissions": [
    "geolocation",
    "storage",
    "http://api.openweathermap.org/"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png" },
    "manifest_version": 2
}
